<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewData["Title"] - Sequence Alignment</title>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.3.7/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.3.7/css/bootstrap-theme.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css">
    <link href="~/css/HomeStyles.min.css" rel="stylesheet" />
</head>
<body>
    <div class="btn-group btn-group-justified">
        <div class="btn-group">
            <button type="button" class="btn btn-nav">
                <span class="glyphicon glyphicon-home"></span>
                <p>Home</p>
            </button>
        </div>
        <div class="btn-group">
            <button type="button" class="btn btn-nav">
                <span class="glyphicon glyphicon-align-center"></span>
                <p>Alignment</p>
            </button>
        </div>
        <div class="btn-group">
            <button type="button" class="btn btn-nav">
                <span class="glyphicon glyphicon-wrench"></span>
                <p>Serives</p>
            </button>
        </div>
        <div class="btn-group">
            <button type="button" class="btn btn-nav">
                <span class="glyphicon glyphicon-globe"></span>
                <p>Web API</p>
            </button>
        </div>
        <div class="btn-group">
            <button type="button" class="btn btn-nav">
                <span class="glyphicon glyphicon-info-sign"></span>
                <p>About</p>
            </button>
        </div>
        <div class="btn-group">
            <button type="button" class="btn btn-nav">
                <span class="glyphicon glyphicon-pencil"></span>
                <p>Signup</p>
            </button>
        </div>
    </div>



    @RenderBody()





    <div class="footer-clean">
        <footer>
            <div class="container">
                <div class="row">
                    <div class="col-md-3 col-sm-4 item">
                        <h3>Services</h3>
                        <ul>
                            <li><a href="#">DNA Alignment</a></li>
                            <li><a href="#">Generate Sequences</a></li>
                            <li><a href="#">Clean Sequences</a></li>
                        </ul>
                    </div>
                    <div class="col-md-3 col-sm-4 item">
                        <h3>About</h3>
                        <ul>
                            <li><a href="#">Collage </a></li>
                            <li><a href="#">Team</a></li>
                            <li><a href="#">Legacy</a></li>
                        </ul>
                    </div>
                    <div class="col-md-4 col-sm-4 item">
                        <h3>Developed &amp; Designed</h3>
                        <ul>
                            <li><a href="#">Ahmed Ramadan</a></li>
                            <li><a href="#">Amr Tawfik</a></li>
                            <li><a href="#">Mahmoud Zakaria</a></li>
                        </ul>
                    </div>
                    <div class="col-md-2 item social">
                        <a href="#"><i class="icon ion-social-facebook"></i></a><a href="#"><i class="icon ion-social-twitter"></i></a>
                        <p class="copyright">MTI . CS © 2017</p>
                    </div>
                </div>
            </div>
        </footer>
    </div>


    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.3.7/js/bootstrap.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/Swiper/3.3.1/js/swiper.jquery.min.js"></script>
    <script src="~/js/NavScript.min.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>

